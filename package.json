{
  "name": "square1-api",
  "version": "0.0.1",
  "description": "API for the lead tracking and resource management system.",
  "scripts": {
    "start": "nodemon ./src/bin/www.js --exec babel-node",
    "prebuild": "sh tools/clean.sh",
    "build": "babel -d ./production ./src --presets es2015,stage-2",
    "lint": "esw src tools --color || exit 0",
    "lint:watch": "npm run lint --watch"
  },
  "engines": {
    "node": "6",
    "npm": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wolfspace/square1-api"
  },
  "author": "Mike Rourke",
  "license": "GPL-3.0",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.28.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "nodemon": "^1.11.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
